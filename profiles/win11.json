{
  "categoryOrder": [
    "Dependencies",
    "UserAuditing",
    "LocalAccountPolicy",
    "Apache",
    "Defensive",
    "ServiceAuditing",
    "OSUpdates",
    "AppUpdates",
    "ProhibitedFiles",
    "UnwantedSoftware",
    "MalwarePersistence",
    "UncategorizedOS"
  ],
  "moduleToggles": {
    "SpoofOSUpdates": true,
    "Apache": true
  },
  "args": {
    "MinPasswordLength": 12,
    "PasswordHistory": 24,
    "LockoutThreshold": 5,
    "BrowserPolicyTarget": "AllUsers",
    "UpdateAutoReboot": false,
    "ScanExclusions": [
      "C:\\Windows\\WinSxS"
    ]
  }
}
